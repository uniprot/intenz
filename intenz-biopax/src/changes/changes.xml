<?xml version="1.0" encoding="UTF-8"?>

<document xmlns="http://maven.apache.org/changes/1.0.0"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://maven.apache.org/changes/1.0.0 http://maven.apache.org/plugins/maven-changes-plugin/xsd/changes-1.0.0.xsd">

    <properties>
        <title>IntEnz BioPAX change log</title>
        <author email="rafael.alcantara@ebi.ac.uk">Rafael Alc√°ntara</author>
    </properties>
    <body>
        <release version="1.0.6" date="2014-06-23"
            description="Dependency updates (new Rhea data model)">
            <action dev="rafalcan" type="update">
                Updated to latest versions of parent, intenz-mapper-4.6.0,
                rhea-biopax-1.4.8 and rhea-mapper-3.8.2 (whic use the new Rhea
                data model).
            </action>
        </release>
    	<release version="1.0.5" date="2012-11-30" description="IUBMB flags">
            <action dev="rafalcan" type="add">
                Added IUBMB flag to catalysis as a comment.
            </action>
    	</release>
    	<release version="1.0.4" date="2012-10-08"
    		description="Dependency updates">
        	<action dev="rafalcan" type="update">
        		Updated to latest version of intenz-core, intenz-mapper,
        		rhea-biopax - which fixes issue with citations model -,
        		rhea-mapper and junit.
        	</action>
    	</release>
    	<release version="1.0.3" date="2012-05-22"
    		description="Fixed base and RDF IDs">
        	<action dev="rafalcan" type="fix">
        		RDF IDs set to URIs.
        	</action>
        	<action dev="rafalcan" type="fix">
        		Fixed XML base (it was appearing as Rhea's).
        	</action>
        	<action dev="rafalcan" type="update">
        		Updated to latest version of intenz-mapper.
        	</action>
    	</release>
    	<release version="1.0.2" date="2010-04-14" description="Dependency update">
        	<action dev="rafalcan" type="update">
        		Updated to latest version of parent (intenz) intenz-core,
                intenz-mapper, rhea-mapper and rhea-biopax.
        	</action>
            <action dev="rafalcan" type="fix">
                Fixed RDF IDs of publication xrefs, in some cases might be wrong.
            </action>
    	</release>
    	<release version="1.0.1" date="2009-10-12" description="EC numbers added">
        	<action dev="rafalcan" type="add">
        		Added EC numbers to all reactions (beforehand, for some reason
        		only abstract reactions got EC numbers).
        	</action>
    	</release>
    	<release version="1.0" date="2009-05-07" description="First release">
        	<action dev="rafalcan" type="add">
        		Added BioPAX module to IntEnz in order to export it as XML
                including Rhea reactions.
        	</action>
    	</release>
        
    </body>
</document>
